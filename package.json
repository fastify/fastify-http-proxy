    "simple-get": "^4.0.0",
    "snazzy": "^9.0.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "standard": "^16.0.4",
    "tap": "^15.1.6",
    "tsd": "^0.19.1",
